{
  "name": "Lendaddon",
  "version": "0.1",
  "description": "Lendaddon, because automation is the future.",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "lendaddon@kunicmarko20"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": ["releaseButton.js"],
      "css": ["ReleaseButton.css"],
      "matches": ["https://github.com/*"]
    },
    {
      "js": ["releasePage.js"],
      "matches": ["https://github.com/*"]
    },
    {
      "js": ["replaceTicketNumber.js"],
      "matches": ["https://github.com/*"]
    }
  ]
}
