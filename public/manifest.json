{
  "name": "Lendaddon",
  "version": "0.1",
  "description": "Lendaddon, because automation is the future.",
  "manifest_version": 2,
  "icons": {
    "16": "logo.png"
  },
  "content_scripts": [
    {
      "js": ["releaseButton.js"],
      "css": ["ReleaseButton.css"],
      "matches": ["https://github.com/*"]
    },
    {
      "js": ["releasePage.js"],
      "matches": ["https://github.com/*"]
    },
    {
      "js": ["replaceTicketNumber.js"],
      "matches": ["https://github.com/*"]
    }
  ]
}
